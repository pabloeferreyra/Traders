@model IEnumerable<Traders.Models.FuturesUpdateViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Movimientos Futuros</h1>

<p>
    <a asp-action="Create">Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ModifDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gain)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ModifDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gain)
            </td>
        </tr>
}
    </tbody>
</table>
