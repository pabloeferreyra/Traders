{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css",
      "destination": "wwwroot/css/fontawesome"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "cdnjs",
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    }
  ]
}